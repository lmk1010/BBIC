<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping package="com.bbic.domain">
<class name="UserInfo" table="user_info">
<id name="usercode" column="usercode">
<generator class="assigned"></generator>
</id>
<property name="username" column="username"></property>
<property name="userimg" column="userimg"></property>
<property name="userauthority" column="userauthority"></property>
<property name="usercatagory" column="usercatagory"></property>
<property name="userdepartment" column="userdepartment"></property>
<property name="usercompany" column="usercompany"></property>

</class> 


</hibernate-mapping>